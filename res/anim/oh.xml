<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
	android:shareInterpolator="false" android:repeatCount="5"
	android:repeatMode="reverse">


	<!-- <alpha android:fromAlpha="1.0" android:toAlpha="0.0" android:startOffset="50" 
		android:duration="70" android:fillAfter="false" /> -->

	<scale android:fromXScale="1.0" android:toXScale="0.8"
		android:fromYScale="1.0" android:toYScale="0.8" android:fillAfter="false"
		android:duration="120" android:pivotX="50%" android:pivotY="50%" />


	<!-- <translate android:fromXDelta="1" android:toXDelta="5" android:fromYDelta="1" 
		android:toYDelta="5" android:fillAfter="false" android:duration="120" /> -->

	<set android:interpolator="@android:anim/accelerate_interpolator"
		android:startOffset="120">

		<scale android:fromXScale="1.0" android:toXScale="1.2"
			android:fromYScale="1.0" android:toYScale="1.2" android:fillAfter="false"
			android:duration="120" android:pivotX="50%" android:pivotY="50%" />

		<set android:interpolator="@android:anim/accelerate_interpolator"
			android:startOffset="120">

			<rotate android:fromDegrees="0" android:toDegrees="20"
				android:pivotX="50%" android:pivotY="50%" android:duration="120"
				android:fillAfter="false" />

			<set android:interpolator="@android:anim/accelerate_interpolator"
				android:startOffset="120">

				<rotate android:fromDegrees="0" android:toDegrees="-20"
					android:pivotX="50%" android:pivotY="50%" android:duration="120"
					android:fillAfter="false" />
			</set>

		</set>

	</set>




</set>		
